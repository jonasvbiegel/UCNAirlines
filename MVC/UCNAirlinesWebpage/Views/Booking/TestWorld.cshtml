@model UCNAirlinesWebpage.Models.SeatPassenger


<h1>Passenger Information</h1>

<ul>
    @foreach (var passenger in Model.Passengers)
    {
        <li>@passenger.FirstName @passenger.LastName</li>
    }
</ul>