@model List<UCNAirlinesWebpage.Models.Passenger>

<h3>PassengerInfo</h3>

<table>
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Seat</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var p in Model)
		{
			<tr>
				<td>@p.FirstName</td>
				<td>@p.LastName</td>
				<td>@p.SelectedSeat</td>
			</tr>
		}
	</tbody>
</table>
